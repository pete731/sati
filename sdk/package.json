{
  "name": "@sati/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for SATI - Solana Agent Trust Infrastructure",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**",
    "README.md",
    "idl.json",
    "types.ts"
  ],
  "scripts": {
    "build": "tsdown",
    "prepublishOnly": "pnpm build",
    "dev": "tsdown --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "solana",
    "sati",
    "agent-infrastructure",
    "erc-8004",
    "ap2"
  ],
  "author": "tenequm",
  "license": "CC0-1.0",
  "peerDependencies": {
    "@coral-xyz/anchor": "^0.32.1",
    "@solana-program/token": "^0.8.0",
    "@solana/kit": "^5.0.0"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.32.1",
    "@solana-program/token": "^0.8.0",
    "@solana/kit": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "tsdown": "^0.16.1",
    "typescript": "^5.7.3"
  }
}
